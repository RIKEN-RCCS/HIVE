
print('Render Obj')

local camera = Camera()
camera:SetScreenSize(1024, 1024)
camera:SetFilename('image.jpg')

camera:Look{
	from = {0,0,300},
	to   = {0,0,0},
	up   = {0,1,0},
	fov  = 60
}


local obj = ObjLoader()
obj:Load('bunny.obj')

local model = PolygonModel()
model:Create(obj)

render {camera, model}

